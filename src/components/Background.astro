---
import Plum from '~/components/bgs/Plum.astro'
import Dot from '~/components/bgs/Dot.astro'
import Rose from '~/components/bgs/Rose.astro'

interface Props {
  type: string
}

const { type } = Astro.props
---

{type === 'plum' && <Plum />}
{type === 'dot' && <Dot />}
{type === 'rose' && <Rose />}
